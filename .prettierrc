{
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 90,
  "semi": true,
  "singleQuote": true,
  "parser": "typescript",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser":"html"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "parser":"scss"
      }
    }
  ]
}
